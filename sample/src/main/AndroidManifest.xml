<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smartmatic.smarteco.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sample">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".theming.ThemingActivity"
            android:label="@string/title_theming"
            />

        <activity
            android:name=".button.ButtonActivity"
            android:label="@string/title_button" />

        <activity
            android:name=".feedback.FeedbackActivity"
            android:label="@string/title_button" />

    </application>

</manifest>